fn main() {
  i := 0;
  loop {
    if i >= 10 { break }

    printf("i = %d\n", i);
    i = i + 1;
  }
  
  return 0;
}

fn array_loop() {
  xs := [1, 2, 3];
  loop x in xs {
    printf("x = %d\n", x);
  }
}

fn while_loop() {
  i := 0;
  loop when i < 10 {
    printf("i = %d\n", i);
    i = i + 1;
  }
}
