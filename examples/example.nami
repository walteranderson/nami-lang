fn add(a: int, b: int) int {
    return a + b;
}

fn main() int {
    printf("Hello World!\n");

    res: int = add(1, 1);
    printf("add(1, 1) = %d\n", res);

    if res == 2 {
        printf("Math works!\n");
    } else {
        printf("Math doesnt work...\n");
        return 1;
    }

    if res >= 2 {
        printf("%d is greater than or equal to %d\n", res, 2);
    }

    printf("See ya!\n");
    return 0;
}

